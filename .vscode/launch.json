  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "command": "npm run serve",
      "name": "npm run serve",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run serve:prod",
      "name": "npm run serve:prod",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}

    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "panzoom": "^9.4.3",
    "qr-scanner": "^1.4.2",
    "qrcode.vue": "^1.7.0",
    "semver": "^7.6.3",
    "shlex": "^2.1.2",
    "sortablejs": "^1.15.2",

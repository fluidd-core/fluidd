    "@jaames/iro": "^5.5.2",
    "axios": "^1.13.2",
    "consola": "^3.4.2",
    "dompurify": "^3.3.1",
    "echarts": "^5.6.0",
    "echarts-gl": "^2.0.9",
    "hls.js": "^1.6.15",
    "ipaddr.js": "^2.3.0",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "marked": "^17.0.1",
    "marked-base-url": "^1.1.8",
    "md5": "^2.3.0",
    "monaco-editor": "^0.54.0",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "panzoom": "^9.4.3",
    "path-browserify": "^1.0.1",
    "qr-scanner": "^1.4.2",
    "qrcode.vue": "^1.7.0",
    "semver": "^7.7.3",
    "shlex": "^3.0.0",
    "sortablejs": "^1.15.6",
